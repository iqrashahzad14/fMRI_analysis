{
  "Name": "handUp MVPA",
  "BIDSModelVersion": "1.0.0",
  "Description": "contrasts for the hand Up dataset used for mvpa",
  "Input": {
    "task": [
      "handUp"
    ],
    "space": [
      "IXI549Space"
    ]
  },
  "Nodes": [
    {
      "Level": "Run",
      "Name": "run_level",
      "GroupBy": [
        "run",
        "session",
        "subject"
      ],
      "Model": {
        "X": [
          "trial_type.handUp_pinkyThumb",
          "trial_type.handUp_fingerWrist",
          "trial_type.response",
          "trans_?",
          "rot_?",
          "non_steady_state_outlier*",
          "motion_outlier*"
        ],
        "HRF": {
          "Variables": [
            "trial_type.handUp_pinkyThumb",
            "trial_type.handUp_fingerWrist",
            "trial_type.response"
          ],
          "Model": "spm"
        },
        "Type": "glm",
        "Options": {
          "HighPassFilterCutoffHz": 0.0078,
          "Mask": {
            "suffix": [
              "mask"
            ],
            "desc": [
              "brain"
            ]
          }
        },
        "Software": {
          "SPM": {
            "SerialCorrelation": "FAST"
          }
        }
      },
      "DummyContrasts": {
        "Contrasts": [
          "trial_type.handDown_pinkyThumb",
          "trial_type.handDown_fingerWrist"
        ],
        "Test": "t"
      }
    }   
  ]
}